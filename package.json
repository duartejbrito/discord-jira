{
  "name": "discord-jira",
  "version": "1.0.0",
  "description": "A Discord bot helper integrating with Jira",
  "author": {
    "email": "sytertzp@gmail.com",
    "name": "Duarte 'SyTeR' Brito"
  },
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/duartejbrito/discord-jira",
  "repository": {
    "type": "git",
    "url": "https://github.com/duartejbrito/discord-jira.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev-docker": "npm run build && docker compose down && docker compose --env-file .env.local up -d",
    "format": "prettier --write \"**/*.{json,ts}\"",
    "lint": "eslint . --ext ts --fix",
    "build": "tsup src/index.ts --minify",
    "start": "node .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "discord.js": "^14.21.0",
    "dotenv": "^16.6.1",
    "dotenv-expand": "^11.0.7",
    "node-fetch": "^3.3.2",
    "node-schedule": "^2.1.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@swc/core": "^1.13.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.120",
    "@types/node-schedule": "^2.1.8",
    "@types/validator": "^13.15.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.6.0",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.1.2",
    "tsup": "^6.7.0",
    "tsx": "^3.14.0",
    "typescript": "^4.9.5"
  }
}
